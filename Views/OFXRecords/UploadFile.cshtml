@addTagHelper *, Vereyon.Web.FlashMessage
@{
    ViewData["Title"] = "UploadFile";
}
<h2>UploadFile</h2>

@using(Html.BeginForm("Import","OFXRecords", FormMethod.Post, new { enctype="multipart/form-data"}))
{
    <flash dismissable="true" bootstrap-version="3"/>
    <br />
    <div>
        @Html.TextBox("file", "", new {  type= "file"}) <br />
        <input type="submit" value="Upload" />
        @ViewBag.Message
    </div>
}

